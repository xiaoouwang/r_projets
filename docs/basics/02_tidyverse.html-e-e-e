<h1><span class="section-number">2. </span>Tidyverse worflow<a class="headerlink" href="#Tidyverse-worflow" title="Lien permanent vers ce titre">¶</a></h1>
<div class="nbinput nblast docutils container">
<div class="prompt highlight-none notranslate"><div class="highlight"><pre><span></span>[36]:
</pre></div>
</div>
<div class="input_area highlight-r notranslate"><div class="highlight"><pre>
<span></span><span class="nf">options</span><span class="p">(</span><span class="n">warn</span><span class="o">=</span><span class="m">-1</span><span class="p">)</span>
<span class="nf">options</span><span class="p">(</span><span class="n">messages</span><span class="o">=</span><span class="m">-1</span><span class="p">)</span>
<span class="nf">options</span><span class="p">(</span><span class="n">tidyverse.quiet</span> <span class="o">=</span> <span class="kc">TRUE</span><span class="p">)</span>
<span class="nf">options</span><span class="p">(</span><span class="n">dplyr.summarise.inform</span> <span class="o">=</span> <span class="kc">FALSE</span><span class="p">)</span>
<span class="n">shhh</span> <span class="o">&lt;-</span> <span class="n">suppressWarnings</span> <span class="c1"># It's a library, so shhh!</span>
<span class="nf">shhh</span><span class="p">(</span><span class="nf">library</span><span class="p">(</span><span class="n">tidyverse</span><span class="p">))</span>
</pre></div>
</div>
</div>
<div class="nbinput nblast docutils container">
<div class="prompt highlight-none notranslate"><div class="highlight"><pre><span></span>[7]:
</pre></div>
</div>
<div class="input_area highlight-r notranslate"><div class="highlight"><pre>
<span></span><span class="c1">## ---------------------------------------------------------------------------------------------------</span>
<span class="n">vow.dur</span> <span class="o">&lt;-</span> <span class="nf">read.table</span><span class="p">(</span><span class="s">"https://bit.ly/2Iw7kn7"</span><span class="p">,</span> <span class="n">header</span><span class="o">=</span><span class="kc">TRUE</span><span class="p">,</span> <span class="n">sep</span><span class="o">=</span><span class="s">"\t"</span><span class="p">)</span>
</pre></div>
</div>
</div>
<div class="nbinput docutils container">
<div class="prompt highlight-none notranslate"><div class="highlight"><pre><span></span>[23]:
</pre></div>
</div>
<div class="input_area highlight-r notranslate"><div class="highlight"><pre>
<span></span><span class="c1"># main dplyr functions</span>
<span class="n">vow.dur</span> <span class="o">%&gt;%</span>
  <span class="nf">filter</span><span class="p">(</span><span class="n">context</span> <span class="o">==</span> <span class="s">"voiceless"</span> <span class="o">&amp;</span> <span class="n">Vow_dur_ms</span> <span class="o">&gt;</span> <span class="nf">mean</span><span class="p">(</span><span class="n">Vow_dur_ms</span><span class="p">,</span> <span class="n">na.rm</span> <span class="o">=</span> <span class="kc">TRUE</span><span class="p">))</span> <span class="o">%&gt;%</span>
  <span class="nf">arrange</span><span class="p">(</span><span class="nf">desc</span><span class="p">(</span><span class="n">Vow_dur_ms</span><span class="p">))</span> <span class="o">%&gt;%</span>
<span class="c1">#   select(contains("_")) %&gt;%</span>
<span class="c1"># explain why some vow_dur &gt; mean</span>
  <span class="nf">mutate</span><span class="p">(</span><span class="n">mean_vow_dur</span> <span class="o">=</span> <span class="nf">mean</span><span class="p">(</span><span class="n">Vow_dur_ms</span><span class="p">,</span> <span class="n">na.rm</span> <span class="o">=</span> <span class="kc">TRUE</span><span class="p">))</span> <span class="o">%&gt;%</span>
  <span class="nf">rename</span><span class="p">(</span><span class="n">vowel_duration</span> <span class="o">=</span> <span class="n">Vow_dur_ms</span><span class="p">)</span>
</pre></div>
